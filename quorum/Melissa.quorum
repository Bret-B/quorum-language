/**
  * Expected output:
  *
  * Error: callMe failed
  */

use Libraries.Language.Errors.Error

class Main
    action Main
        check
            callMe(50)
        detect e of type Error
            e:PrintStackTrace()
        end
    end

    action callMe(integer depth)
        if depth <= 25
            //return now
            print depth
            Error err
            alert(err)
        end
    
        callMe(depth - 1)
    end
end