class Melissa

action Main
        boolean hasRemoved = false
        integer count = 0
        integer size = 5
        repeat until count < size
            Object item
            if item:Equals(item) then
                
                hasRemoved = true
                size = size - 1
            end else then
                count = count + 1
            end
        end
end

end