class Loops
    action Main
        integer x = 3
        integer i = 1
        integer my = 0
        repeat 3 times
            i = i + 1
        end
        repeat while x > 0
            integer m = 0
            m = m + 1
            x = x - 1
        end
        repeat while i < x
            i = i + 1
        end
        integer bill = 2
        repeat from 2 to 3
           i = i - 1
           bill = bill + 1
        end
        repeat 2 times
            i = i + 1
        end
    end
end