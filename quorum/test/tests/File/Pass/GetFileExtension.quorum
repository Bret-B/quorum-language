use Libraries.System.File
use Libraries.System.DateTime

class Main
    action Main
        File dot
        dot:SetRelativePath(".")
        text dotExtension = dot:GetFileExtension() // should be empty string

        File relDir
        relDir:SetRelativePath("./")
        text relDirExtension = relDir:GetFileExtension() // should be empty string (as './' is not a file)

        File noExt
        noExt:SetAbsolutePath("/Users/jeff")
        text noExtExtension = noExt:GetFileExtension() // should be empty string

        File hasExt
        hasExt:SetAbsolutePath("C:\Program Files\file.ext")
        text hasExtExtension = hasExt:GetFileExtension() // should be "ext"

        print dotExtension // 
        print relDirExtension //
        print noExtExtension //
        print hasExtExtension // ext
    end
end