use Libraries.System.File
use Libraries.System.FileRandomAccess

class Main
    action Main
        File f
        f:SetRelativePath("test/tests/FileRandomAccess/Resources/readThirdChar")
        FileRandomAccess r
        r:OpenForRandomAccess(f)
        
        r:SetPosition(2)
        text thirdChar = r:Read(1)

        print thirdChar // should be 'e'
    end
end