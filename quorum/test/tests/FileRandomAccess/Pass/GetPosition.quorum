use Libraries.System.File
use Libraries.System.FileRandomAccess

class Main
    action Main
        File f
        f:SetRelativePath("test/tests/FileReader/Resources/singleLine")
        FileRandomAccess r
        r:OpenForRandomAccess(f)
        
        // Read the entire file, should be at position 21
        r:Read()
        number position = r:GetPosition()
        
        // Set position to 2, should print 2
        r:SetPosition(2)
        number newPosition = r:GetPosition()

        print position
        print newPosition
    end
end