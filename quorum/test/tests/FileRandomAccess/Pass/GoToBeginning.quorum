use Libraries.System.File
use Libraries.System.FileRandomAccess

class Main
    action Main
        File f
        f:SetRelativePath("../../../test/tests/FileReader/Resources/mixReads")
        FileRandomAccess r
        r:OpenForRandomAccess(f)
        r:Read()
        // Set position to 2, should print 0
        r:GoToBeginning()
        number position = r:GetPosition()

        print position
    end
end