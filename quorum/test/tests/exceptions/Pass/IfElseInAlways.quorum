/*
Expected output:
detect
always
*/

use Libraries.Language.Errors.all

class Main
    action Main
        check
            integer k = cast(integer, "3.7")
        detect e of type CastError
            print "detect"
        always
            integer z = 906
            if z = 907
                print "shouldn't be here"
            else
                print "always"
            end
        end
    end
end