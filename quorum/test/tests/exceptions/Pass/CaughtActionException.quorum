/**
  * Expected output:
  *
  * Error: callMe failed
  */

use Libraries.Language.Errors.Error

class Main
    action Main
        check
            callMe(50)
        detect e of type Error
            print "Error: " + e:GetErrorMessage()
        end
    end

    action callMe(integer depth)
        if depth <= 25
            alert("callMe failed")
        end
    
        callMe(depth - 1)
    end
end