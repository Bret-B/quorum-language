/*
Expected output:
detect
always
*/

use Libraries.Language.Errors.all

class Main
    action Main
        check
            integer k = cast(integer, "3.7")
        detect e of type CastError
            integer z = 11
            if z = 10
                print "shouldn't be here"
            elseif z = 11
                print "detect"
            else
                print "shouldn't be here either"
            end
        always
            print "always"
        end
    end
end