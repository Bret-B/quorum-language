/**
Expected output:
error
always
*/

use Libraries.Language.Errors.Error
use Libraries.Language.Errors.CastError
use Libraries.Language.Errors.OutOfBoundsError

class Main
    action Main
        check
            alert("error")
        detect e of type OutOfBoundsError
            print "OutOfBoundsError"
        detect j of type CastError
            print "CastError"
        detect k
            print k:GetErrorMessage()
        always
            print "always"
        end
    end
end