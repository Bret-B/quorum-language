/**
Expected output:
always
alert
*/

use Libraries.Language.Errors.Error

class Main
    action Main
        check
            check
                suspiciousMethod()
            detect j of type Error
                alert(j) // rethrow
            always
                print "always"
            end
        detect e
            print e:GetErrorMessage()
        end
    end

    action suspiciousMethod
        alert("alert")
    end
end