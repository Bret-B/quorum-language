/**
Expected output:
A
C
inner always 2
outer always
D
*/

use Libraries.Language.Errors.Error

class Main
    action Main
        check
            alert("A")
        detect e of type Error
            output e:GetErrorMessage()
            check
                alert("C")
            detect f of type Error
                 output f:GetErrorMessage()
            always
                output "inner always 2"
            end
        always
            output "outer always"
            check
                alert("D")
            detect e
                output e:GetErrorMessage()
            end
        end
    end
end