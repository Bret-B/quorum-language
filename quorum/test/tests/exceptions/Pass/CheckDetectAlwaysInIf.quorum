/*
Expected output:
detect
always
*/

use Libraries.Language.Errors.all

class Main
    action Main
        integer z = 2 * 2
        if z = 4
            check
                alert("detect")
            detect e
                print e:GetErrorMessage()
            always
                print "always"
            end
        else
            print "shouldn't be here"
        end
    end
end