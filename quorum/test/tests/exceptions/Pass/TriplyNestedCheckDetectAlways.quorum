/*
Expected output:
detect 1
detect 2
check 3
always 3
always 2
always 1
*/

use Libraries.Language.Errors.all

class Main
    action Main
        check
            alert("some error")
        detect e
            print "detect 1"
            check
                alert("detect 2")
            detect j
                print j:GetErrorMessage()
                check
                    print "check 3"
                detect e
                    print "shouldn't be here"
                always
                    print "always 3"
                end
            always
                print "always 2"
            end
        always
            print "always 1"
        end
    end
end