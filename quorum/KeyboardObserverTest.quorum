use Libraries.Accessibility.all

class KeyboardObserverTest is KeyboardObserver
    action ReceiveEvent(KeyboardEvent e)
        if e:GetAction() = "KeyPress"
            output ("Category: " + e:GetCategory() + " Action: " + e:GetAction() + " Key: " + e:GetKey() )
        elseif e:GetAction() = "KeyRelease"
            output e:GetDescription()
        else
            output "other keyboard event"
        end
        //output "hello from KeyboardObserver"
        //output "You pressed " + e:GetKey()
    end
end