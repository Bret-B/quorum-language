class Main
    action Main
       integer a = 10
       boolean result
       if odd(a) then
            result = true
       end
       else then
            result = false
       end

       print result
    end

    action odd (integer a) returns boolean
        if a mod 2 = 0 then
            return false
        end
        else then
            return true
        end
    end
end
