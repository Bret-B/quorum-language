package Science.Tests

use Libraries.Testing.Test
use Libraries.Compute.Statistics.DataFrame
use Libraries.Compute.Statistics.Tests.CompareGroups
use Libraries.Compute.Statistics.Reporting.CompareGroupsResult

/*
    This class tests a two-sample wilcoxon signed-rank test
*/
class Compare2DependentRankedGroupsTest is Test

    on create
        SetName("Calculate Compare2DependentRankedGroups")
    end

    action Run
        ColumnCalculationTest framer
        DataFrame frame = framer:GetDefaultDataFrame()

        CompareGroups compare
        compare:Paired(true)
        compare:Ranked(true)
        compare:AddColumn(0)
        compare:AddColumn(1)
        frame:Calculate(compare)
        
        CompareGroupsResult result = compare:GetResult()
        Check(result:GetTestStatistic(), 1.7153800557404717)
        Check(result:GetProbabilityValue(), 0.08627556958252947)
        Check(result:GetEffectSize(), 0.7003009752518536)
    end
end