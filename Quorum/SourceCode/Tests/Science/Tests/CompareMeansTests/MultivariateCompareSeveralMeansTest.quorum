package Science.Tests

use Libraries.Testing.Test
use Libraries.Compute.Statistics.DataFrame
use Libraries.Compute.Statistics.Tests.CompareMeans
use Libraries.Compute.Statistics.Reporting.CompareMeansResult

/*
    This class tests a one-way manova 
*/
class MultivariateCompareSeveralMeansTest is Test

    on create
        SetName("Calculate MultivariateCompareSeveralMeansTest")
    end

    action Run
        DataFrame frame 
        frame:LoadFromCommaSeparatedValue("y,x1,x2,x3,factor1,factor2
        1,3,-4,4,a,c
        2,4,-9,9,a,c
        3,4,-18,18,a,c
        6,7,-19,19,a,d
        9,8,-16,16,a,d
        15,17,-2,2,b,c
        10,2,1,12,b,c
        3,4,2,13,b,d
        5,6,3,15,b,d")
        frame:AddSelectedColumns("y,x1,x2,x3")
        frame:AddSelectedFactors("factor1")

        CompareMeans compare
        frame:Calculate(compare)
        
        CompareMeansResult result = compare:GetResult()
        text stat = result:GetTestStatisticsTable():GetValue("factor1"):GetKeyIterator():Next()
        Check(result:GetDegreesOfFreedomTable():GetValue("factor1"):GetValue("factor1"), 1.0)
        Check(result:GetTestStatisticsTable():GetValue("factor1"):GetValueIterator():Next(), 0.0467435942460233)
        Check(result:GetTestStatisticsTable():GetValue("factor1"+stat):GetValueIterator():Next(), 20.39330567385872)
        Check(result:GetDegreesOfFreedomTable():GetValue("factor1"):GetValue("numerator"), 4.0)
        Check(result:GetDegreesOfFreedomTable():GetValue("factor1"):GetValue("denominator"), 4.0)
        Check(result:GetProbabilityValuesTable():GetValue("factor1"):GetValueIterator():Next(), 0.006350624704905217)
        Check(result:GetEffectSizesTable():GetValue("factor1"):GetValue("partial-eta-squared"), 0.9532564057539767)
    end
end