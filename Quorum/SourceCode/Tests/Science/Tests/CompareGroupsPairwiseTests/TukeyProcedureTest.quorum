package Science.Tests

use Libraries.Testing.Test
use Libraries.Compute.Statistics.DataFrame
use Libraries.Compute.Statistics.Tests.CompareGroupsPairwise
use Libraries.Compute.Statistics.Reporting.CompareGroupsResult

/*
    This class tests a tukey procedure on pairwise t-tests
*/
class TukeyProcedureTest is Test

    on create
        SetName("Calculate CompareGroupsPairwise - Tukey Procedure")
    end

    action Run
        ColumnCalculationTest framer
        DataFrame frame = framer:GetDefaultDataFrame()

        CompareGroupsPairwise compare
        compare:UseTukeyCorrection()
        compare:AddColumn(0)
        compare:AddColumn(1)
        compare:AddColumn(2)
        frame:Calculate(compare)
        
        // Compare 0-1
        CompareGroupsResult result0 = compare:GetResults():Get(0)
        Check(result0:GetTestStatistic(), 0.47359318999796673)
        Check(result0:GetProbabilityValue(), 0.9403814678068939)

        // Compare 0-2
        CompareGroupsResult result1 = compare:GetResults():Get(1)
        Check(result1:GetTestStatistic(), 7.036241679969795)
        Check(result1:GetProbabilityValue(), 5.955996183658607e-4)

        // Compare 1-2
        CompareGroupsResult result2 = compare:GetResults():Get(2)
        Check(result2:GetTestStatistic(), 7.5098348699677615)
        Check(result2:GetProbabilityValue(), 3.8830988779747777e-4)
    end
end