package Science.Tests

use Libraries.Testing.Test
use Libraries.Compute.Statistics.DataFrame
use Libraries.Compute.Statistics.Tests.CompareGroupsPairwise
use Libraries.Compute.Statistics.Reporting.CompareGroupsResult

/*
    This class tests a games-howell procedure on pairwise t-tests
*/
class GamesHowellProcedureTest is Test

    on create
        SetName("Calculate CompareGroupsPairwise - Games-Howell Procedure")
    end

    action Run
        ColumnCalculationTest framer
        DataFrame frame = framer:GetDefaultDataFrame()

        CompareGroupsPairwise compare
        compare:UseLiberalCorrection(true)
        compare:AssumeEqualVariances(false)
        compare:AddColumn(0)
        compare:AddColumn(1)
        compare:AddColumn(2)
        frame:Calculate(compare)
        
        // Compare 0-1
        CompareGroupsResult result0 = compare:GetResults():Get(0)
        Check(result0:GetTestStatistic(), 0.3854541397819923)
        Check(result0:GetProbabilityValue(), 0.9220910084942867)

        // Compare 0-2
        CompareGroupsResult result1 = compare:GetResults():Get(1)
        Check(result1:GetTestStatistic(), 4.681076771500411)
        Check(result1:GetProbabilityValue(), 0.0025167188968470144)

        // Compare 1-2
        CompareGroupsResult result2 = compare:GetResults():Get(2)
        Check(result2:GetTestStatistic(), 5.027825732483661)
        Check(result2:GetProbabilityValue(), 0.0015862922958967962)
    end
end