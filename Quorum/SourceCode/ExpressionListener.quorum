package Libraries.Language.Interpreter

use Libraries.Language.Compile.QuorumSourceListener
use Libraries.Language.Compile.Context.IntegerContext
use Libraries.Language.Compile.Context.AdditionContext
use Libraries.Language.Compile.Context.NumberContext
use Libraries.Language.Compile.Context.MultiplicationContext
use Libraries.Language.Compile.Context.ParenthesisContext
use Libraries.Language.Compile.Context.PackageContext
use Libraries.Language.Compile.Context.UseContext
use Libraries.Language.Compile.QualifiedName

class ExpressionListener is QuorumSourceListener
    action EnterInteger(IntegerContext context) 
        output "Entered integer with value " + context:value
    end
    action ExitInteger(IntegerContext context) 
        output "Exited integer with value " + context:value
    end

    action EnterNumber(NumberContext context) 
        output "Entered number with value " + context:value
    end
    action ExitNumber(NumberContext context) 
        output "Exited number with value " + context:value
    end

    action EnterAddition(AdditionContext context) 
        output "Entered Addition"
    end
    action ExitAddition(AdditionContext context) 
        output "Exited Addition"
    end

    action EnterMultiplication(MultiplicationContext context) 
        output "Entered Mult"
    end
    action ExitMultiplication(MultiplicationContext context) 
        output "Exited Mult"
    end

    action EnterParenthesisExpression(ParenthesisContext context) 
        output "("
    end
    action ExitParenthesisExpression(ParenthesisContext context) 
        output ")"
    end

    action EnterPackageRule(PackageContext context) 
        output "Enter package at " + context:name:GetStaticKey()
    end
    action ExitPackageRule(PackageContext context) 
        output "Exit package at " + context:name:GetStaticKey()
    end

    action EnterUse(UseContext context) 
        output "Enter Use at " + context:name:GetStaticKey()
    end
    action ExitUse(UseContext context) 
        output "Exit Use at " + context:name:GetStaticKey()
    end
end