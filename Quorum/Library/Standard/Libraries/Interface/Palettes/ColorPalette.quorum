    action SetBackgroundColor(text controlType, ColorGroup value)
        ColorPalette palette = GetControlPalette(controlType)
        if palette = undefined
            ColorPalette newPalette
            palette = newPalette
            SetControlPalette(controlType, palette)
        end
        palette:SetBackgroundColor(value)
    end

    action GetBackgroundColor(text controlType) returns ColorGroup
