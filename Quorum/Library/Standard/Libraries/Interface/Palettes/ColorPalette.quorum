    public constant text CHECKBOX = "Checkbox"
    public constant text SCROLL_PANE = "ScrollPane"
    public constant text TAB_BAR = "TabBar"

    HashTable<text, ColorPalette> controlPalettes

    action HasControlPalette(text name) returns boolean
        return controlPalettes:HasKey(name)
    end

    action GetControlPalette(text name) returns ColorPalette
        if HasControlPalette(name)
            return controlPalettes:GetValue(name)
        else
            return undefined
        end
    end

    action SetControlPalette(text name, ColorPalette palette)
        controlPalettes:Set(name, palette)
    end

    action Copy returns ColorPalette
        ColorPalette new
        new:SetBackgroundColor(backgroundColor)
        new:SetForegroundColor(foregroundColor)
        new:SetFontColor(fontColor)
        new:SetBorderColor(borderColor)
        new:SetMouseOverColor(mouseOverColor)
        new:SetMouseOverFontColor(mouseOverFontColor)
        new:SetMouseDownColor(mouseDownColor)
        new:SetMouseDownFontColor(mouseDownFontColor)
        new:SetSelectionColor(selectionColor)
        new:SetUnfocusedSelectionColor(unfocusedSelectionColor)
        new:SetSelectionFontColor(selectionFontColor)
        new:SetGlowColor(glowColor)
        new:SetMouseOverGlowColor(mouseOverGlowColor)
        new:SetMouseDownGlowColor(mouseDownGlowColor)
        new:SetIconColor(iconColor)
        new:SetAccentColor(accentColor)

        Iterator<text> iterator = controlPalettes:GetKeyIterator()
        repeat while iterator:HasNext()
            text key = iterator:Next()
            new:SetControlPalette(key, controlPalettes:GetValue(key))
        end

        return new
    end

    action CopyControlPalette(text paletteName) returns ColorPalette
        if controlPalettes:HasKey(paletteName)
            return controlPalettes:GetValue(paletteName):Copy()
        end
        return undefined
    end

