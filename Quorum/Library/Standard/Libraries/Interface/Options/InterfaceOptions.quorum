    // HashTables for color & number properties and interface options.
    HashTable<text, ColorGroup> colorProperties
    HashTable<text, number> numberProperties
    HashTable<text, InterfaceOptions> interfaceOptions
    HashTable<text, text> customProperty

    // The dummy number only exists so we can set the constant NaN value.
    number dummyNumber = 0
    constant number NOT_A_NUMBER = dummyNumber:GetNotANumberValue()

    action HasCustomProperty(text name) returns boolean
        return customProperty:HasKey(name)
    end

    action GetCustomProperty(text name) returns text
        if HasCustomProperty(name)
            return customProperty:GetValue(name)
        else
            return undefined
        end
    end

    action SetCustomProperty(text name, text value)
        customProperty:Set(name, value)
    end

    action HasInterfaceOptions(text name) returns boolean
        return interfaceOptions:HasKey(name)
    end

    action GetInterfaceOptions(text name) returns InterfaceOptions
        if HasInterfaceOptions(name)
            return interfaceOptions:GetValue(name)
        else
            return undefined
        end
    end

    action SetInterfaceOptions(text name, InterfaceOptions palette)
        interfaceOptions:Set(name, palette)
    end

    action GetColorProperty(text key) returns ColorGroup
        if colorProperties:HasKey(key)
            return colorProperties:GetValue(key)
        else
            return undefined
        end
    end

    action SetColorProperty(text key, ColorGroup color)
        colorProperties:Set(key, color)
    end

    action GetNumberProperty(text key) returns number
        if numberProperties:HasKey(key)
            return numberProperties:GetValue(key)
        else
            // For missing color values we return undefined, but for numbers we return NaN instead.
            return NOT_A_NUMBER
        end
    end

    action SetNumberProperty(text key, number value)
        numberProperties:Set(key, value)
    end

    action HasNumberProperty(text key) returns boolean
        return GetNumberProperty(key):IsNotANumber() = false
    end

    action CopyInterfaceOptions(text key) returns InterfaceOptions
        if interfaceOptions:HasKey(key)
            return interfaceOptions:GetValue(key):Copy()
        end
        return undefined
    end

    action Copy returns InterfaceOptions
        InterfaceOptions new
        Iterator<text> propertyIterator = colorProperties:GetKeyIterator()
        repeat while propertyIterator:HasNext()
            text key = propertyIterator:Next()
            new:SetColorProperty(key, colorProperties:GetValue(key))
        end

        Iterator<text> numberIterator = numberProperties:GetKeyIterator()
        repeat while numberIterator:HasNext()
            text key = numberIterator:Next()
            new:SetNumberProperty(key, numberProperties:GetValue(key))
        end

        Iterator<text> controlIterator = interfaceOptions:GetKeyIterator()
        repeat while controlIterator:HasNext()
            text key = controlIterator:Next()
            new:SetInterfaceOptions(key, interfaceOptions:GetValue(key))
        end

        return new
    end

