    // HashTables for color properties and interface options.
    HashTable<text, ColorGroup> colorProperties
    HashTable<text, InterfaceOptions> interfaceOptions

    action HasInterfaceOptions(text name) returns boolean
        return interfaceOptions:HasKey(name)
    end

    action GetInterfaceOptions(text name) returns InterfaceOptions
        if HasInterfaceOptions(name)
            return interfaceOptions:GetValue(name)
        else
            return undefined
        end
    end

    action SetInterfaceOptions(text name, InterfaceOptions palette)
        interfaceOptions:Set(name, palette)
    end

    action GetColorProperty(text key) returns ColorGroup
        if colorProperties:HasKey(key)
            return colorProperties:GetValue(key)
        else
            return undefined
        end
    end

    action SetColorProperty(text key, ColorGroup color)
        colorProperties:Set(key, color)
    end

    action CopyInterfaceOptions(text key) returns InterfaceOptions
        if interfaceOptions:HasKey(key)
            return interfaceOptions:GetValue(key):Copy()
        end
        return undefined
    end

    action Copy returns InterfaceOptions
        InterfaceOptions new
        Iterator<text> propertyIterator = colorProperties:GetKeyIterator()
        repeat while propertyIterator:HasNext()
            text key = propertyIterator:Next()
            new:SetColorProperty(key, colorProperties:GetValue(key))
        end

        Iterator<text> controlIterator = interfaceOptions:GetKeyIterator()
        repeat while controlIterator:HasNext()
            text key = controlIterator:Next()
            new:SetInterfaceOptions(key, interfaceOptions:GetValue(key))
        end

        return new
    end

