package Libraries.Game.Graphics.Models
use Libraries.Containers.Array
class Animations
    Array<Animation> animations
    Array<AnimationMeshData> animationMeshData
    action HasAnimations returns boolean
        if animations = undefined or animations:IsEmpty()
            return false
        else
            return true
        end
    end
    action GetAnimations returns Array<Animation>
        return animations
    end

    action SetAnimations(Array<Animation> animations)
        me:animations = animations
    end

    action GetAnimationMeshData returns Array<AnimationMeshData>
        return animationMeshData
    end

    action SetAnimationMeshData(Array<AnimationMeshData> animationMeshData)
        me:animationMeshData = animationMeshData
    end

end
