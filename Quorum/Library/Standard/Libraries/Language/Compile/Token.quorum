package Libraries.Language.Compile

class Token
    integer tokenCategory = -1
    integer startIndex = -1
    integer stopIndex = -1
    text tokenText = ""

    action GetTokenCategory returns integer
        return tokenCategory
    end

    action SetTokenCategory(integer category)
        me:tokenCategory = category
    end

    action GetStartIndex returns integer
        return startIndex
    end

    action GetStopIndex returns integer
        return stopIndex
    end

    action GetText returns text
        return tokenText
    end

    action SetStartIndex(integer index)
        me:startIndex = index
    end

    action SetStopIndex(integer index)
        me:stopIndex = index
    end

    action SetText(text value)
        me:tokenText = value
    end
end
