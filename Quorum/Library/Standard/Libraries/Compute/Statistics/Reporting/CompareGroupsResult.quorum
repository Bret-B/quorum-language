package Libraries.Compute.Statistics.Reporting

use Libraries.Containers.Array
use Libraries.Compute.Statistics.Inputs.ColumnInput
use Libraries.Compute.Statistics.DataFrameColumn
use Libraries.Compute.Statistics.DataFrame

/*
    This class represents data that comes back from a CompareGroups calculation.

    Attribute: Author Andreas Stefik
*/
class CompareGroupsResult is StatisticalTestResult, ColumnInput
    Array<DataFrameColumn> groups // The columns used in this calculation.
    text factor = ""
    /*
        Sets the factor (if used) in this calculation.

        Attribute: Parameter variable is factor header and unique factor.
    */
    action SetFactor(text factor)
        me:factor = factor
    end

    /*
        Returns the factor (if used) in this calculation.

        Attribute: returns factor header and unique factor.
    */
    action GetFactor returns text
        return factor
    end

    /* Used in CompareNGroupsResult */
    action GetPairwiseResults returns Array<CompareGroupsResult>
        return undefined
    end

    action GetGroups returns Array<DataFrameColumn>
        return groups
    end

    action SetGroups(Array<DataFrameColumn> groups)
        me:groups = groups
    end

    /* Overwritten in Compare2GroupsResult and Compare1GroupsResult */
    action GetMean1 returns number
        number nan = 0
        return nan:GetNotANumberValue()
    end

    /* Overwritten in Compare2GroupsResult and Compare1GroupsResult */
    action GetMean2 returns number
        number nan = 0
        return nan:GetNotANumberValue()
    end

    /* Overwritten in Compare2GroupsResult and Compare1GroupsResult */
    action GetVariance1 returns number
        number nan = 0
        return nan:GetNotANumberValue()
    end

    /* Overwritten in Compare2GroupsResult and Compare1GroupsResult */
    action GetVariance2 returns number
        number nan = 0
        return nan:GetNotANumberValue()
    end

    action Save(text path)

    end

    /*
        This action summarizes the result and lists it informally.
    */
    action GetSummary returns text
        return ""
    end

    /*
        This action summarizes the result and places it into formal academic language, in 
        APA format.

        For more information: https://apastyle.apa.org/instructional-aids/numbers-statistics-guide.pdf
    */
    action GetFormalSummary returns text
        return ""
    end
end