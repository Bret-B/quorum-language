use Libraries.Compute.Statistics.DataFrame
use Libraries.Interface.Controls.Charts.PieChart

DataFrame frame
frame:Load("Library/Tests/Charts/Resources/ChartTestData.csv")
frame:AddSelectedFactors("Factor1")

PieChart chart = frame:PieChart()
chart:SetTitle("Set Color Palette To Magma")
chart:SetSubtitle("One Factor")
chart:SetColorPaletteToMagma()
chart:Display()
chart:Save("chart.svg")