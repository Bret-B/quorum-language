use Libraries.Compute.Vector2

package Libraries.Game.Collision

class ManifoldPoint2D
    public Vector2 localPoint
    public number normalImpulse = 0
    public number tangentImpulse = 0

    action Set(ManifoldPoint2D point)
        me:localPoint:Set(point:localPoint)
        me:normalImpulse = point:normalImpulse
        me:tangentImpulse = point:tangentImpulse
    end
end