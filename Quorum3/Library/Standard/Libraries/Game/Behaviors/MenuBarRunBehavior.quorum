package Libraries.Game.Behaviors

use Libraries.Game.Behaviors.Behavior
use Libraries.Interface.Events.BehaviorEvent
use Libraries.Interface.Controls.MenuBar

class MenuBarRunBehavior is Behavior
    action Run(BehaviorEvent event)
        if event:GetItem() is MenuBar
            MenuBar bar = cast(MenuBar, event:GetItem())
            bar:RunSelection()
        end
    end
end