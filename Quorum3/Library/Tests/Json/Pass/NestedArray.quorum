use Libraries.Data.Formats.JavaScriptObjectNotation

class Main 
    action Main
            JavaScriptObjectNotation data
            data:Add("item1", "a")
            data:Add("item2", "b")
            data:Add("item3", "c")

            JavaScriptObjectNotation json
            json:Add("data1", "A")
            json:Add("data2", "B")
            json:Add("detail", data)


            CheckResult(json:ToText())
    end

    action CheckResult(text result)
        text answer = ""
        text cr = answer:GetCarriageReturn()
        text dq = answer:GetDoubleQuote()
        answer = "{"+cr+"    "+dq+"data1"+dq+": "+dq+"A"+dq+", "+cr+"    "+dq+"data2"+dq+": "+dq+"B"+dq+", "+cr+"    "+dq+"detail"+dq+":     {"+cr+"        "+dq+"item1"+dq+": "+dq+"a"+dq+", "+cr+"        "+dq+"item2"+dq+": "+dq+"b"+dq+", "+cr+"        "+dq+"item3"+dq+": "+dq+"c"+dq+cr+"    }"+cr+"}"
        if answer = result
            output "pass"
        else
            output "fail"
        end
    end

end